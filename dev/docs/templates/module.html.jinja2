{% extends "default/module.html.jinja2" %}
{% block title %}{{ module.modulename }}aa FSWLIB documentation{% endblock %}

{% block nav_members %}
    {% if module.members %}
        <h2>FSWLIB documentation</h2>
        {{ nav_ordered_members(module.members.values()) }}
    {% endif %}
{% endblock %}

{% defaultmacro nav_ordered_members(members) %}
    <ul class="memberlist">

        {# METHODS FIRST #}
        {% for m in members if is_public(m) and m.kind == "variable" | trim %}
            <li>
                <a class="method" href="#{{ m.qualname }}">{{ m.name }}</a>
            </li>
        {% endfor %}

        {# PROPERTIES SECOND #}
        {% for m in members if is_public(m) and m.kind == "property" | trim %}
            <li>
                <a class="property" href="#{{ m.qualname }}">{{ m.name }}</a>
            </li>
        {% endfor %}

        {# OTHER MEMBERS LAST (including classes, modules, etc.) #}
        {% for m in members if is_public(m) and m.kind not in ["variable", "property"] | trim %}
            <li>
                {% if m.kind == "class" %}
                    <a class="class" href="#{{ m.qualname }}">{{ m.qualname }}</a>
                    {% if m.own_members %}
                        {{ nav_ordered_members(m.own_members) | indent(12) }}
                    {% endif %}
                {% elif m.kind == "module" %}
                    <a class="module" href="#{{ m.name }}">{{ m.name }}</a>
                {% elif m.name == "__init__" %}
                    <a class="function" href="#{{ m.qualname }}">{{ m.qualname.split(".")[-2] }}</a>
                {% else %}
                    <a class="{{ m.kind }}" href="#{{ m.qualname }}">{{ m.name }}</a>
                {% endif %}
            </li>
        {% endfor %}

    </ul>
{% enddefaultmacro %}
