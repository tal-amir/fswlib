{% extends "default/module.html.jinja2" %}
{% block title %}{{ module.modulename }}aa FSWLIB documentation{% endblock %}

{% block nav_members %}
    {% if module.members %}
        <h2>aa FSWLIB Documentation</h2>
        {{ nav_members(module.members.values()) }}
    {% endif %}
{% endblock %}

{% block module_contents %}
    {% for m in module.flattened_own_members if is_public(m) | trim %}
        <section id="{{ m.qualname or m.name }}">
            {{ member(m) }}
            {% if m.kind == "class" %}
                {# METHODS #}
                {% for m in m.own_members if m.kind == "method" and is_public(m) | trim %}
                    <div id="{{ m.qualname }}" class="classattr">
                        {{ member(m) }}
                    </div>
                {% endfor %}
                {# PROPERTIES #}
                {# OTHER NON-CLASS MEMBERS #}
                {# INHERITED MEMBERS (unchanged) #}
            {% endif %}
        </section>
    {% endfor %}
{% endblock %}