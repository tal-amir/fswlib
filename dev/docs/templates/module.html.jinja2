{% extends "default/module.html.jinja2" %}
{% block title %}{{ module.modulename }}FSWLIB documentation{% endblock %}

{% block nav_members %}
    {% if module.members %}
        <h2>FSWLIB documentation</h2>

        {# METHODS #}
        {{ nav_members(module.members.values() | selectattr("kind", "equalto", "method")) }}

        {# PROPERTIES #}
        {{ nav_members(module.members.values() | selectattr("kind", "equalto", "property")) }}

        {# OTHER (fields, classes, etc.) #}
        {{ nav_members(module.members.values() | rejectattr("kind", "in", ["method", "property"])) }}

    {% endif %}
{% endblock %}
